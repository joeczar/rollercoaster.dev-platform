{
  "name": "backend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "backend/src",
  "targets": {
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ],
      "command": "bun run build",
      "options": {
        "cwd": "backend"
      }
    },
    "serve": {
      "command": "bun run start",
      "options": {
        "cwd": "backend"
      }
    }
  },
  "tags": []
} 